{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport Swal from 'sweetalert2';\n// import alert from \"@/plugins/alert\";\n//   import helpers from \"@/helpers/helpers\";\nexport default {\n  name: \"AppUserProfile\",\n  data() {\n    return {\n      data: {\n        name: \"\",\n        email: \"\",\n        username: \"\",\n        phone: \"\",\n        address: \"\",\n        avatar: \"\",\n        recentActivities: []\n      },\n      loading: true\n    };\n  },\n  methods: {\n    async fetchData() {\n      try {\n        const response = await axios.get(\"/api/example\"); // Fetch user data\n        this.data = response.data;\n      } catch (error) {\n        console.error(\"Error fetching data:\", error);\n      } finally {\n        this.loading = false;\n      }\n    },\n    editProfile() {\n      // Redirect to edit profile page or open a modal\n      alert(\"Edit profile feature coming soon!\");\n    },\n    logout() {\n      try {\n        axios.get('clear-cache').then(response => {\n          console.log(response);\n          this.$router.push({\n            name: 'HomePage'\n          });\n        });\n      } catch (error) {\n        console.log(error);\n      }\n      alert(\"Logging out...\");\n    }\n  },\n  mounted() {\n    this.fetchData();\n  }\n};","map":{"version":3,"names":["axios","Swal","name","data","email","username","phone","address","avatar","recentActivities","loading","methods","fetchData","response","get","error","console","editProfile","alert","logout","then","log","$router","push","mounted"],"sources":["C:\\laravel\\HMS\\hotel-booking-frontend\\src\\components\\AppUserProfile.vue"],"sourcesContent":["<template>\n    <div class=\"profile-page\">\n      <div class=\"card\">\n        <div class=\"card-header text-center\">\n          <h2>User Profile</h2>\n        </div>\n        <div class=\"card-body\">\n          <div class=\"profile-header\">\n            <h3 class=\"profile-name\">{{ data.name || 'John Doe' }}</h3>\n            <p class=\"profile-email\">{{ data.email || 'johndoe@example.com' }}</p>\n          </div>\n\n          <div v-if=\"loading\" class=\"loading\">\n            <p>Loading profile...</p>\n          </div>\n\n          <div v-else>\n            <div class=\"profile-details\">\n              <h4>Details</h4>\n              <ul>\n                <li><strong>Username:</strong> {{ data.username || 'N/A' }}</li>\n                <li><strong>Phone:</strong> {{ data.phone || 'N/A' }}</li>\n                <li><strong>Address:</strong> {{ data.address || 'N/A' }}</li>\n              </ul>\n            </div>\n\n            <!-- <div class=\"profile-activity\">\n              <h4>Recent Activity</h4>\n              <ul>\n                <li v-for=\"(activity, index) in data.recentActivities\" :key=\"index\">\n                  {{ activity }}\n                </li>\n              </ul>\n            </div> -->\n          </div>\n        </div>\n\n        <div class=\"card-footer text-center\">\n          <button class=\"btn btn-primary\" @click=\"editProfile\">Edit Profile</button>\n          <button class=\"btn btn-secondary\" @click=\"logout\">Logout</button>\n        </div>\n      </div>\n    </div>\n  </template>\n\n  <script>\nimport axios from \"axios\";\nimport Swal from 'sweetalert2';\n// import alert from \"@/plugins/alert\";\n//   import helpers from \"@/helpers/helpers\";\n  export default {\n    name: \"AppUserProfile\",\n    data() {\n      return {\n        data: {\n          name: \"\",\n          email: \"\",\n          username: \"\",\n          phone: \"\",\n          address: \"\",\n          avatar: \"\",\n          recentActivities: [],\n        },\n        loading: true,\n      };\n    },\n    methods: {\n      async fetchData() {\n        try {\n          const response = await axios.get(\"/api/example\"); // Fetch user data\n          this.data = response.data;\n        } catch (error) {\n          console.error(\"Error fetching data:\", error);\n        } finally {\n          this.loading = false;\n        }\n      },\n      editProfile() {\n        // Redirect to edit profile page or open a modal\n        alert(\"Edit profile feature coming soon!\");\n      },\n      logout() {\n        try {\n            axios.get('clear-cache').then((response)=>{\n                console.log(response)\n               this.$router.push({ name: 'HomePage' });\n\n\n            });\n        } catch (error) {\n            console.log(error);\n        }\n        alert(\"Logging out...\");\n      },\n    },\n    mounted() {\n      this.fetchData();\n    },\n  };\n  </script>\n\n  <style scoped>\n  .profile-page {\n    max-width: 600px;\n    margin: 0 auto;\n    padding: 20px;\n  }\n\n  .card {\n    border: 1px solid #ddd;\n    border-radius: 8px;\n    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n  }\n\n  .card-header {\n    background-color: #007bff;\n    color: white;\n    padding: 15px;\n    border-bottom: 1px solid #ddd;\n  }\n\n  .profile-header {\n    text-align: center;\n    margin-bottom: 20px;\n  }\n\n  .profile-avatar {\n    width: 100px;\n    height: 100px;\n    border-radius: 50%;\n    object-fit: cover;\n    margin-bottom: 10px;\n  }\n\n  .profile-name {\n    font-size: 1.5rem;\n    font-weight: bold;\n  }\n\n  .profile-email {\n    color: #555;\n  }\n\n  .profile-details,\n  .profile-activity {\n    margin-bottom: 20px;\n  }\n\n  .profile-details h4,\n  .profile-activity h4 {\n    margin-bottom: 10px;\n    font-size: 1.2rem;\n    color: #333;\n  }\n\n  .profile-details ul,\n  .profile-activity ul {\n    list-style: none;\n    padding: 0;\n  }\n\n  .profile-details li,\n  .profile-activity li {\n    margin-bottom: 5px;\n  }\n\n  .card-footer {\n    padding: 15px;\n  }\n\n  .btn {\n    margin: 0 5px;\n  }\n  </style>\n"],"mappings":";AA8CA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAC9B;AACA;AACE,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLA,IAAI,EAAE;QACJD