{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport Swal from 'sweetalert2';\nimport helpers from \"@/helpers/helpers\";\n// import alert from \"@/plugins/alert\";\n//   import helpers from \"@/helpers/helpers\";\nexport default {\n  name: \"AppUserProfile\",\n  data() {\n    return {\n      data: {\n        username: \"\",\n        phone: \"\",\n        address: \"\",\n        avatar: \"\",\n        recentActivities: []\n      },\n      loading: false,\n      name: \"\",\n      email: \"\",\n      currentPw: \"\",\n      loading_text: \"\",\n      newPassowrd: \"\",\n      confirmPassword: \"\"\n    };\n  },\n  methods: {\n    async helpersfetchData() {\n      try {\n        const response = await axios.get(\"/api/example\"); // Fetch user data\n        this.data = response.data;\n      } catch (error) {\n        console.error(\"Error fetching data:\", error);\n      } finally {\n        this.loading = false;\n      }\n    },\n    editProfile() {\n      // Redirect to edit profile page or open a modal\n      alert(\"Edit profile feature coming soon!\");\n    },\n    logout() {\n      try {\n        Swal.fire({\n          text: 'Are you sure you want to logout?',\n          icon: 'warning',\n          showCancelButton: true,\n          confirmButtonText: 'Yes'\n        }).then(result => {\n          if (result.isConfirmed) {\n            axios.get('clear-cache').then(response => {\n              console.log(response);\n              this.$router.push({\n                name: 'HomePage'\n              });\n            });\n          }\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async checkPassword() {\n      this.loading = true;\n      this.loading_text = 'loading.....';\n      const response = await helpers.checkPw(this.currentPw);\n      if (response) {\n        this.loading = false;\n        this.loading_text = '';\n      } else {\n        this.loading_text = 'enter the correct password';\n      }\n    },\n    async checkPw() {\n      console.log('here');\n    },\n    async changePw() {}\n  },\n  async mounted() {\n    const userData = await helpers.getUserData();\n    this.email = userData.email;\n    this.name = userData.name;\n    const changepw = document.getElementById('changepw');\n    console.log(changepw);\n    changepw.style.display = 'none';\n  }\n};","map":{"version":3,"names":["axios","Swal","helpers","name","data","username","phone","address","avatar","recentActivities","loading","email","currentPw","loading_text","newPassowrd","confirmPassword","methods","helpersfetchData","response","get","error","console","editProfile","alert","logout","fire","text","icon","showCancelButton","confirmButtonText","then","result","isConfirmed","log","$router","push","checkPassword","checkPw","changePw","mounted","userData","getUserData","changepw","document","getElementById","style","display"],"sources":["C:\\laravel\\Hotel-Booking-System\\hotel-booking-frontend\\src\\components\\AppUserProfile.vue"],"sourcesContent":["<template>\n    <div class=\"profile-page\">\n      <div class=\"card\">\n        <div class=\"card-header text-center\">\n          <h2>User Profile</h2>\n        </div>\n        <div class=\"card-body\">\n          <div class=\"profile-header\">\n            <h3 class=\"profile-name\">{{ name || 'John Doe' }}</h3>\n            <p class=\"profile-email\">{{ email || 'johndoe@example.com' }}</p>\n          </div>\n\n          <div v-if=\"loading\" class=\"loading\">\n            loading..\n          </div>\n          <div v-else>\n            <div class=\"profile-details\">\n              <h4>Details</h4>\n              <ul>\n                <li><strong>Username:</strong> {{ data.username || 'N/A' }}</li>\n                <li><strong>Phone:</strong> {{ data.phone || 'N/A' }}</li>\n                <li><strong>Address:</strong> {{ data.address || 'N/A' }}</li>\n              </ul>\n            </div>\n\n          </div>\n        </div>\n\n        <div class=\"card-footer text-center\">\n            <button type=\"button\" class=\"btn btn-primary\" data-bs-toggle=\"modal\" data-bs-target=\"#exampleModal\">\n      edit\n    </button>\n          <!-- <button class=\"btn btn-primary\" @click=\"editProfile\">Edit Profile</button> -->\n          <button class=\"btn btn-secondary\" @click=\"logout\">Logout</button>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"modal fade\" id=\"exampleModal\" tabindex=\"-1\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\n    <div class=\"modal-dialog\">\n        <div class=\"modal-content\">\n            <div class=\"modal-header\">\n                <h5 class=\"modal-title\" id=\"exampleModalLabel\">Edit profile</h5>\n                <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n            </div>\n            <div class=\"modal-body\">\n                    <div class=\"mb-3\">\n                        <label for=\"name\" class=\"form-label\">Name</label>\n                        <input type=\"text\" v-model=\"name\" class=\"form-control\" id=\"name\" name=\"name\" required>\n                    </div>\n                    <div class=\"mb-3\">\n                        <label for=\"email\" class=\"form-label\">Email</label>\n                        <input type=\"email\" v-model=\"email\" class=\"form-control\" id=\"email\" name=\"email\" required>\n                    </div>\n                    <div class=\"mb-3\">\n                        <label for=\"password\" class=\"form-label\">current password (type your password to change the password)</label>\n                        <input type=\"password\" class=\"form-control\" v-model=\"currentPw\" id=\"password\" name=\"password\" @input=\"checkPassword\" required>\n                    </div>\n\n                    <div v-if=\"loading\" class=\"load-spinner\">\n                       {{ loading_text }}\n                    </div>\n\n                    <div v-else id=\"changepw\" class=\"loading-spinner \">\n                        <div class=\"mb-3\">\n                            <label for=\"newPassowrd\" class=\"form-label\">new password</label>\n                            <input type=\"password\" v-model=\"newPassowrd\" @input=\"checkPw()\" class=\"form-control\" id=\"newPassowrd\" name=\"newPassowrd\" required>\n                        </div>\n\n                        <div class=\"mb-3\">\n                            <label for=\"confirmPassword\" class=\"form-label\">confirm password</label>\n                            <input type=\"password\" v-model=\"confirmPassword\" @input=\"checkPw()\" class=\"form-control\" id=\"confirmPassword\" name=\"newPassowrd\" required>\n                        </div>\n                    </div>\n                    <button v-if=\"hidebutton\" type=\"submit\" @click=\"changePw()\" class=\"btn btn-primary\">Submit</button>\n            </div>\n        </div>\n    </div>\n</div>\n  </template>\n\n  <script>\nimport axios from \"axios\";\nimport Swal from 'sweetalert2';\nimport helpers from \"@/helpers/helpers\";\n// import alert from \"@/plugins/alert\";\n//   import helpers from \"@/helpers/helpers\";\n  export default {\n    name: \"AppUserProfile\",\n    data() {\n      return {\n        data: {\n          username: \"\",\n          phone: \"\",\n          address: \"\",\n          avatar: \"\",\n          recentActivities: [],\n        },\n        loading: false,\n        name: \"\",\n        email: \"\",\n        currentPw:\"\",\n        loading_text:\"\",\n        newPassowrd:\"\",\n        confirmPassword:\"\"\n      };\n    },\n    methods: {\n      async helpersfetchData() {\n        try {\n          const response = await axios.get(\"/api/example\"); // Fetch user data\n          this.data = response.data;\n        } catch (error) {\n          console.error(\"Error fetching data:\", error);\n        } finally {\n          this.loading = false;\n        }\n      },\n      editProfile() {\n        // Redirect to edit profile page or open a modal\n        alert(\"Edit profile feature coming soon!\");\n      },\n      logout() {\n        try {\n                Swal.fire({\n                    text: 'Are you sure you want to logout?',\n                    icon: 'warning',\n                    showCancelButton: true,\n                    confirmButtonText: 'Yes',\n            }).then((result) => {\n                if (result.isConfirmed) {\n                    axios.get('clear-cache').then((response)=>{\n                        console.log(response)\n                        this.$router.push({ name: 'HomePage' });\n                    });\n                }\n            });\n\n        } catch (error) {\n            console.log(error);\n        }\n      },\n   async checkPassword()\n    {\n        this.loading = true;\n        this.loading_text= 'loading.....';\n        const response=  await  helpers.checkPw(this.currentPw);\n        if(response)\n        {\n            this.loading = false;\n            this.loading_text = '';\n        }\n        else{\n            this.loading_text= 'enter the correct password';\n        }\n    },\n\n    async checkPw()\n    {\n        console.log('here')\n    },\n\n    async changePw()\n    {\n\n    }\n},\n\n\n   async mounted() {\n        const userData = await helpers.getUserData();\n        this.email = userData.email;\n        this.name = userData.name;\n        const changepw = document.getElementById('changepw');\n        console.log(changepw)\n        changepw.style.display = 'none';\n    },\n  };\n  </script>\n\n  <style scoped>\n  .profile-page {\n    max-width: 600px;\n    margin: 0 auto;\n    padding: 20px;\n  }\n\n  .card {\n    border: 1px solid #ddd;\n    border-radius: 8px;\n    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n  }\n\n  .card-header {\n    background-color: #007bff;\n    color: white;\n    padding: 15px;\n    border-bottom: 1px solid #ddd;\n  }\n\n  .profile-header {\n    text-align: center;\n    margin-bottom: 20px;\n  }\n\n  .profile-avatar {\n    width: 100px;\n    height: 100px;\n    border-radius: 50%;\n    object-fit: cover;\n    margin-bottom: 10px;\n  }\n\n  .profile-name {\n    font-size: 1.5rem;\n    font-weight: bold;\n  }\n\n  .profile-email {\n    color: #555;\n  }\n\n  .profile-details,\n  .profile-activity {\n    margin-bottom: 20px;\n  }\n\n  .profile-details h4,\n  .profile-activity h4 {\n    margin-bottom: 10px;\n    font-size: 1.2rem;\n    color: #333;\n  }\n\n  .profile-details ul,\n  .profile-activity ul {\n    list-style: none;\n    padding: 0;\n  }\n\n  .profile-details li,\n  .profile-activity li {\n    margin-bottom: 5px;\n  }\n\n  .card-footer {\n    padding: 15px;\n  }\n\n  .btn {\n    margin: 0 5px;\n  }\n\n  /* .loading-spinner {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  height: 100px;\n  font-size: 1.5em;\n} */\n  </style>\n"],"mappings":";AAkFA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAC9B,OAAOC,OAAM,MAAO,mBAAmB;AACvC;AACA;AACE,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLA,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVC,gBAAgB,EAAE;MACpB,CAAC;MACDC,OAAO,EAAE,KAAK;MACdP,IAAI,EAAE,EAAE;MACRQ,KAAK,EAAE,EAAE;MACTC,SAAS,EAAC,EAAE;MACZC,YAAY,EAAC,EAAE;MACfC,WAAW,EAAC,EAAE;MACdC,eAAe,EAAC;IAClB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,gBAAgBA,CAAA,EAAG;MACvB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMlB,KAAK,CAACmB,GAAG,CAAC,cAAc,CAAC,EAAE;QAClD,IAAI,CAACf,IAAG,GAAIc,QAAQ,CAACd,IAAI;MAC3B,EAAE,OAAOgB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAC9C,UAAU;QACR,IAAI,CAACV,OAAM,GAAI,KAAK;MACtB;IACF,CAAC;IACDY,WAAWA,CAAA,EAAG;MACZ;MACAC,KAAK,CAAC,mCAAmC,CAAC;IAC5C,CAAC;IACDC,MAAMA,CAAA,EAAG;MACP,IAAI;QACIvB,IAAI,CAACwB,IAAI,CAAC;UACNC,IAAI,EAAE,kCAAkC;UACxCC,IAAI,EAAE,SAAS;UACfC,gBAAgB,EAAE,IAAI;UACtBC,iBAAiB,EAAE;QAC3B,CAAC,CAAC,CAACC,IAAI,CAAEC,MAAM,IAAK;UAChB,IAAIA,MAA