{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport Swal from 'sweetalert2';\nimport helpers from \"@/helpers/helpers\";\n// import alert from \"@/plugins/alert\";\n//   import helpers from \"@/helpers/helpers\";\nexport default {\n  name: \"AppUserProfile\",\n  data() {\n    return {\n      data: {\n        username: \"\",\n        phone: \"\",\n        address: \"\",\n        avatar: \"\",\n        recentActivities: []\n      },\n      loading: true,\n      name: \"\",\n      email: \"\"\n    };\n  },\n  methods: {\n    async helpersfetchData() {\n      try {\n        const response = await axios.get(\"/api/example\"); // Fetch user data\n        this.data = response.data;\n      } catch (error) {\n        console.error(\"Error fetching data:\", error);\n      } finally {\n        this.loading = false;\n      }\n    },\n    editProfile() {\n      // Redirect to edit profile page or open a modal\n      alert(\"Edit profile feature coming soon!\");\n    },\n    logout() {\n      try {\n        Swal.fire({\n          text: 'Are you sure you want to logout?',\n          icon: 'warning',\n          showCancelButton: true,\n          confirmButtonText: 'Yes'\n        }).then(result => {\n          if (result.isConfirmed) {\n            axios.get('clear-cache').then(response => {\n              console.log(response);\n              this.$router.push({\n                name: 'HomePage'\n              });\n            });\n          }\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    }\n  },\n  async mounted() {\n    const userData = await helpers.getUserData();\n    this.email = userData.email;\n    this.name = userData.name;\n  }\n};","map":{"version":3,"names":["axios","Swal","helpers","name","data","username","phone","address","avatar","recentActivities","loading","email","methods","helpersfetchData","response","get","error","console","editProfile","alert","logout","fire","text","icon","showCancelButton","confirmButtonText","then","result","isConfirmed","log","$router","push","mounted","userData","getUserData"],"sources":["C:\\laravel\\Hotel-Booking-System\\hotel-booking-frontend\\src\\components\\AppUserProfile.vue"],"sourcesContent":["<template>\r\n    <div class=\"profile-page\">\r\n      <div class=\"card\">\r\n        <div class=\"card-header text-center\">\r\n          <h2>User Profile</h2>\r\n        </div>\r\n        <div class=\"card-body\">\r\n          <div class=\"profile-header\">\r\n            <h3 class=\"profile-name\">{{ name || 'John Doe' }}</h3>\r\n            <p class=\"profile-email\">{{ email || 'johndoe@example.com' }}</p>\r\n          </div>\r\n\r\n          <div v-if=\"loading\" class=\"loading\">\r\n          </div>\r\n\r\n          <div v-else>\r\n            <div class=\"profile-details\">\r\n              <h4>Details</h4>\r\n              <ul>\r\n                <li><strong>Username:</strong> {{ data.username || 'N/A' }}</li>\r\n                <li><strong>Phone:</strong> {{ data.phone || 'N/A' }}</li>\r\n                <li><strong>Address:</strong> {{ data.address || 'N/A' }}</li>\r\n              </ul>\r\n            </div>\r\n\r\n            <!-- <div class=\"profile-activity\">\r\n              <h4>Recent Activity</h4>\r\n              <ul>\r\n                <li v-for=\"(activity, index) in data.recentActivities\" :key=\"index\">\r\n                  {{ activity }}\r\n                </li>\r\n              </ul>\r\n            </div> -->\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"card-footer text-center\">\r\n            <button type=\"button\" class=\"btn btn-primary\" data-bs-toggle=\"modal\" data-bs-target=\"#exampleModal\">\r\n      Open Modal\r\n    </button>\r\n          <!-- <button class=\"btn btn-primary\" @click=\"editProfile\">Edit Profile</button> -->\r\n          <button class=\"btn btn-secondary\" @click=\"logout\">Logout</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"modal fade\" id=\"exampleModal\" tabindex=\"-1\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\r\n      <div class=\"modal-dialog\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"modal-header\">\r\n            <h5 class=\"modal-title\" id=\"exampleModalLabel\">Modal title</h5>\r\n            <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\r\n          </div>\r\n          <div class=\"modal-body\">\r\n            This is a Bootstrap modal in Vue.js.\r\n          </div>\r\n          <div class=\"modal-footer\">\r\n            <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">Close</button>\r\n            <button type=\"button\" class=\"btn btn-primary\">Save changes</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n\r\n  <script>\r\nimport axios from \"axios\";\r\nimport Swal from 'sweetalert2';\r\nimport helpers from \"@/helpers/helpers\";\r\n// import alert from \"@/plugins/alert\";\r\n//   import helpers from \"@/helpers/helpers\";\r\n  export default {\r\n    name: \"AppUserProfile\",\r\n    data() {\r\n      return {\r\n        data: {\r\n          username: \"\",\r\n          phone: \"\",\r\n          address: \"\",\r\n          avatar: \"\",\r\n          recentActivities: [],\r\n        },\r\n        loading: true,\r\n        name: \"\",\r\n        email: \"\",\r\n      };\r\n    },\r\n    methods: {\r\n      async helpersfetchData() {\r\n        try {\r\n          const response = await axios.get(\"/api/example\"); // Fetch user data\r\n          this.data = response.data;\r\n        } catch (error) {\r\n          console.error(\"Error fetching data:\", error);\r\n        } finally {\r\n          this.loading = false;\r\n        }\r\n      },\r\n      editProfile() {\r\n        // Redirect to edit profile page or open a modal\r\n        alert(\"Edit profile feature coming soon!\");\r\n      },\r\n      logout() {\r\n        try {\r\n                Swal.fire({\r\n                    text: 'Are you sure you want to logout?',\r\n                    icon: 'warning',\r\n                    showCancelButton: true,\r\n                    confirmButtonText: 'Yes',\r\n            }).then((result) => {\r\n                if (result.isConfirmed) {\r\n                    axios.get('clear-cache').then((response)=>{\r\n                        console.log(response)\r\n                        this.$router.push({ name: 'HomePage' });\r\n                    });\r\n                }\r\n            });\r\n\r\n        } catch (error) {\r\n            console.log(error);\r\n        }\r\n      },\r\n    },\r\n   async mounted() {\r\n        const userData = await helpers.getUserData();\r\n        this.email = userData.email;\r\n        this.name = userData.name;\r\n    },\r\n  };\r\n  </script>\r\n\r\n  <style scoped>\r\n  .profile-page {\r\n    max-width: 600px;\r\n    margin: 0 auto;\r\n    padding: 20px;\r\n  }\r\n\r\n  .card {\r\n    border: 1px solid #ddd;\r\n    border-radius: 8px;\r\n    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n  }\r\n\r\n  .card-header {\r\n    background-color: #007bff;\r\n    color: white;\r\n    padding: 15px;\r\n    border-bottom: 1px solid #ddd;\r\n  }\r\n\r\n  .profile-header {\r\n    text-align: center;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .profile-avatar {\r\n    width: 100px;\r\n    height: 100px;\r\n    border-radius: 50%;\r\n    object-fit: cover;\r\n    margin-bottom: 10px;\r\n  }\r\n\r\n  .profile-name {\r\n    font-size: 1.5rem;\r\n    font-weight: bold;\r\n  }\r\n\r\n  .profile-email {\r\n    color: #555;\r\n  }\r\n\r\n  .profile-details,\r\n  .profile-activity {\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .profile-details h4,\r\n  .profile-activity h4 {\r\n    margin-bottom: 10px;\r\n    font-size: 1.2rem;\r\n    color: #333;\r\n  }\r\n\r\n  .profile-details ul,\r\n  .profile-activity ul {\r\n    list-style: none;\r\n    padding: 0;\r\n  }\r\n\r\n  .profile-details li,\r\n  .profile-activity li {\r\n    margin-bottom: 5px;\r\n  }\r\n\r\n  .card-footer {\r\n    padding: 15px;\r\n  }\r\n\r\n  .btn {\r\n    margin: 0 5px;\r\n  }\r\n  </style>\r\n"],"mappings":";AAkEA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAC9B,OAAOC,OAAM,MAAO,mBAAmB;AACvC;AACA;AACE,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLA,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVC,gBAAgB,EAAE;MACpB,CAAC;MACDC,OAAO,EAAE,IAAI;MACbP,IAAI,EAAE,EAAE;MACRQ,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,gBAAgBA,CAAA,EAAG;MACvB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMd,KAAK,CAACe,GAAG,CAAC,cAAc,CAAC,EAAE;QAClD,IAAI,CAACX,IAAG,GAAIU,QAAQ,CAACV,IAAI;MAC3B,EAAE,OAAOY,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAC9C,UAAU;QACR,IAAI,CAACN,OAAM,GAAI,KAAK;MACtB;IACF,CAAC;IACDQ,WAAWA,CAAA,EAAG;MACZ;MACAC,KAAK,CAAC,mCAAmC,CAAC;IAC5C,CAAC;IACDC,MAAMA,CAAA,EAAG;MACP,IAAI;QACInB,IAAI,CAACoB,IAAI,CAAC;UACNC,IAAI,EAAE,kCAAkC;UACxCC,IAAI,EAAE,SAAS;UACfC,gBAAgB,EAAE,IAAI;UACtBC,iBAAiB,EAAE;QAC3B,CAAC,CAAC,CAACC,IAAI,CAAEC,MAAM,IAAK;UAChB,IAAIA,MAAM,CAACC,WAAW,EAAE;YACpB5B,KAAK,CAACe,GAAG,CAAC,aAAa,CAAC,CAACW,IAAI,CAAEZ,QAAQ,IAAG;cACtCG,OAAO,CAACY,GAAG,CAACf,QAAQ;cACpB,IAAI,CAACgB,OAAO,CAACC,IAAI,CAAC;gBAAE5B,IAAI,EAAE;cAAW,CAAC,CAAC;YAC3C,CAAC,CAAC;UACN;QACJ,CAAC,CAAC;MAEN,EAAE,OAAOa,KAAK,EAAE;QACZC,OAAO,CAACY,GAAG,CAACb,KAAK,CAAC;MACtB;IACF;EACF,CAAC;EACF,MAAMgB,OAAOA,CAAA,EAAG;IACX,MAAMC,QAAO,GAAI,MAAM/B,OAAO,CAACgC,WAAW,CAAC,CAAC;IAC5C,IAAI,CAACvB,KAAI,GAAIsB,QAAQ,CAACtB,KAAK;IAC3B,IAAI,CAACR,IAAG,GAAI8B,QAAQ,CAAC9B,IAAI;EAC7B;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}